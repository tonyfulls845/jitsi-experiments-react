{
  "name": "jitsi-front-experiments",
  "version": "1.0.0",
  "description": "React front application for conferencing with using lib-jitsi-meet",
  "main": "index.tsx",
  "repository": "git@github.com:tonyfulls845/jitsi-experiments-react.git",
  "author": "tonyfulls845",
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc.json resources/ts --ext .ts --ext .tsx",
    "lint:fix": "eslint -c .eslintrc.json resources/ts --ext .ts --ext .tsx --fix",
    "prettier": "prettier --write resources/**/*.{ts,tsx,scss}",
    "build": "webpack --mode production --progress",
    "watch": "webpack serve --mode development --progress",
    "generate-certs": "cd public/certs && openssl genrsa -out ca.key 4096 && openssl req -x509 -new -key ca.key -days 1825 -out ca.crt -config openssl.cfg -subj \"/CN=jitsi-front-experiments-ca\" && openssl genrsa -out cert.key 4096 && openssl req -new -key cert.key -out cert.csr -config openssl.cfg && openssl x509 -req -in cert.csr -CA ca.crt -CAkey ca.key -CAcreateserial -extensions req_ext -extfile openssl.cfg -out cert.crt -days 1825\n",
    "stats": "webpack --mode development --progress --profile --json > webpack-stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@typescript-eslint/typescript-estree": "^4.15.1",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "html-webpack-plugin": "^5.1.0",
    "mini-css-extract-plugin": "^1.3.7",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.2.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "dotenv-webpack": "^6.0.0",
    "lib-jitsi-meet-dist": "^2.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
